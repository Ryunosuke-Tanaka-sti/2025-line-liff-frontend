{
    "trailingSlash": "always",
    "routes": [
        {
            "route": "/",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/404/index.html",
            "statusCode": 404
        },
        "401": {
            "statusCode": 302,
            "redirect": "/.auth/login/google"
        }
    }
}