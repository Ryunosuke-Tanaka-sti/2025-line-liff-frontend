{
  "trailingSlash": "always",
  "routes": [
    {
      "route": "/community/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/community/google/*",
      "allowedRoles": [
        "admin"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/google?post_login_redirect_uri=/community"
    },
    "404": {
      "rewrite": "/404/index.html",
      "statusCode": 404
    }
  }
}